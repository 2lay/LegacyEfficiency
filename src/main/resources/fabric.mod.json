{
  "schemaVersion": 1,
  "id": "legacyefficiency",
  "version": "${version}",
  "name": "Legacy Efficiency",
  "description": "This mod brings back the old block-breaking speed from older Minecraft versions, affecting all block types.",
  "authors": ["2lay", "superehostile"],
  "contact": {
    "repo": "https://github.com/2lay/LegacyEfficiency"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.twolay.legacyefficiency.LegacyEfficiency"
    ]
  },
  "mixins": [
    "LegacyEfficiency.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
